        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Dark"/>
        <StyleInclude Source="avares://MusicStore.Desktop/Assets/Icons.axaml"></StyleInclude>
    </Application.Styles>
</Application>
